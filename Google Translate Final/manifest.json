
{
    "manifest_version": 2,
    "name": "Google translate",
    "description": "Google translate",
    "version": "2.2",
    "default_locale": "en",

    "icons": {
        "16": "icons/logo-16.png",
        "32": "icons/logo-32.png",
        "48": "icons/logo-48.png",
        "96": "icons/logo-96.png"
    },

  "background": {
     "scripts" : ["translate.js" ] 

  },

  "permissions": [
    "contextMenus",
    "storage"
  ]

}